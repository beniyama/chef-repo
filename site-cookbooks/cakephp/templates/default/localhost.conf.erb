LoadModule rewrite_module modules/mod_rewrite.so
ServerName localhost

<VirtualHost *:80>
        ServerName localhost
        DocumentRoot /var/www/cake/HelloCake/webroot/
        RewriteEngine On
        <Directory "/var/www/cake/HelloCake/">
                options FollowSymLinks
                AllowOverride All
        </Directory>
</VirtualHost>
